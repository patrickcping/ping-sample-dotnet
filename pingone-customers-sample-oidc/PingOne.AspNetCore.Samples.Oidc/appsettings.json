{
  "PingOne": {
    "Authentication": {
      "AuthBaseUrl": "https://your-server:9031",
      "ClientId": "your-client-id",
      "Secret": "your-client-secret",
      "ResponseType": "code",
      "RedirectPath": "/callback",
      "PostSignOffRedirectUrl": "/",
      "Scopes": [
        "openid",
        "profile",
        "email",
        "address"
      ]
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
